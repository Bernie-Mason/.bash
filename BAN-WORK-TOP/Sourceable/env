export nuget_temp='C:\Users\Bernie\AppData\Local\Temp\NuGetScratch'
export nuget_plugins_cache='C:\Users\Bernie\AppData\Local\NuGet\plugins-cache'

# Your local package store. 
export local_package_store='/c/dev/packages'
